{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "BigQueryJobEvent",
  "type": "object",
  "required": ["job_id", "timestamp", "job_type", "metadata"],
  "properties": {
    "job_id": {
      "type": "string"
    },
    "timestamp": {
      "type": "string",
      "format": "date-time"
    },
    "job_type": {
      "type": "string",
      "enum": ["QUERY", "BQML"]
    },
    "query": {
      "type": ["string", "null"]
    },
    "metadata": {
      "type": "object",
      "properties": {
        "estimated_bytes": {
          "type": "number",
          "minimum": 0
        }
      },
      "required": ["estimated_bytes"]
    },
    "session_id": {
      "type": ["string", "null"]
    }
  }
}
